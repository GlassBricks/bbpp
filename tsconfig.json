{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "types": [
      "typescript-to-lua/language-extensions",
      "lua-types/5.2"
    ]
  },
  "tstl": {
    "luaTarget": "5.2",
    "noHeader": true,
    "noImplicitSelf": true
  },
  "include": [
    "./**/*"
    //    "./node_modules/factorio-type-kit/factorio.d.ts"
  ]
}
